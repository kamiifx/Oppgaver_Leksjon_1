{
  "root": true,
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2020,
    "ecmaFeatures": {
      "impliedStrict": true,
      "classes": true
    }
  },
  "extends": ["prettier"],
  "plugins": ["html","prettier"],
  "env": {
    "browser": true,
    "node": true,
    "jquery": true,
    "jest": true
  }
}